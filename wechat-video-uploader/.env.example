# https://cloud.siliconflow.cn/account/ak
SILICONFLOW_API_KEY=sk-put-your-key
SILICONFLOW_CHAT_API_URL=https://api.siliconflow.cn/v1/chat/completions

# 集合名称
WECHAT_COLLECTION_NAME=集合名字1

# 视频保存目录
VIDEO_DIR=/Users/user1/video

# 延迟时间配置（单位：毫秒）

# 页面加载后的等待时间
DELAY_PAGE_LOAD=8000

# 视频上传后的处理等待时间
DELAY_VIDEO_PROCESS=15000

# 内容更新后的等待时间
DELAY_CONTENT_UPDATE=5000

# 点击操作后的等待时间
DELAY_AFTER_CLICK=3000

# 发布按钮点击后的等待时间
DELAY_AFTER_PUBLISH=8000

# 上传间隔时间
DELAY_BETWEEN_UPLOADS=5000